type Vec3 = {x: int, y: int, z: int};

func add(a: Vec3, b: Vec3) -> Vec3 {
  return {x: a.x + b.x, y: a.y + b.y};
}

fn calc() {
  var p: [3]Vec3 = [
    {x: 1, y: 2, z: 3},
    {x: 4, y: 5, z: 6},
    {x: 7, y: 8, z: 9},
  ];
  var sum = add(add(p[0],p[1]),p[2]);
  return {x: sum.x/3, y: sum.y/3, z: sum.z/3};
}

asm main() {
  calc();
}
