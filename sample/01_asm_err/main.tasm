// Assembly error examples - various intentional errors for testing

asm @ 0x0000 reset {
    hoge(fuga, fuga);  // Unknown operation `hoge`
    add(z, z);         // More argument required
    add(a0, a1, hoge); // Unknown register name
    call(hoge);        // Undefined label
  a:                  // Defined here
    nop();
  a:                  // Re-defined label
    nop();
    jump(a);
}
